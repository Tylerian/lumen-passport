{
    "name": "tylerian/lumen-passport",
    "keywords": ["lumen", "oauth2", "passport"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jairo Tylera",
            "email": "tylera@jairo.me"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0",
        "laravel/lumen-framework": "~5.6.0|~5.7.0|~5.8.0",
        "tylerian/lumen-support": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.4"
    },
    "autoload": {
        "psr-4": {
            "Tylerian\\Lumen\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tylerian\\Lumen\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "lumen": {
            "providers": [
                "Tylerian\\Lumen\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}